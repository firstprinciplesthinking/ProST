  <Requirements:Requirement xmi:id="_v0ezYHQ1Ee6lW_W_zHdLeg" base_NamedElement="_v0WQgHQ1Ee6lW_W_zHdLeg" id="Req_QAReq_1" text="The requirement should be atomic meaning it describes only a single need, in order to support precise accounting of requirements &amp; identification of needs." base_Class="_v0WQgHQ1Ee6lW_W_zHdLeg"/>
  <ModelElements:Stakeholder xmi:id="_R--mcHQ5Ee6lW_W_zHdLeg" base_Classifier="_R-7jIHQ5Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_-BPawHQ6Ee6lW_W_zHdLeg" base_NamedElement="_-BMXcHQ6Ee6lW_W_zHdLeg" id="Req_QAReq_2" text="The requirement should be  independent meaning it is describe without reference to other requirements, in order to prevent inconsistencies between requirements and enhance understanding of the requirement." base_Class="_-BMXcHQ6Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_B870QHQ7Ee6lW_W_zHdLeg" base_NamedElement="_B85YAHQ7Ee6lW_W_zHdLeg" id="Req_QAReq_3" text="The requirement shall be detailed enough, in order to enable effective development activities." base_Class="_B85YAHQ7Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_Fie-oHQ7Ee6lW_W_zHdLeg" base_NamedElement="_FibUQHQ7Ee6lW_W_zHdLeg" id="Req_QAReq_4" text="The requirement shall be consistent with all other requirements, to avoid competing development activities and/or system disruption." base_Class="_FibUQHQ7Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_vknjsHQ7Ee6lW_W_zHdLeg" base_NamedElement="_vklugHQ7Ee6lW_W_zHdLeg" id="Req_QAReq_5" text="The requirement shall be specific to only the system, to avoid unfeasible development efforts." base_Class="_vklugHQ7Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_-Kx70HQ7Ee6lW_W_zHdLeg" base_NamedElement="_-KwGoHQ7Ee6lW_W_zHdLeg" id="Req_QAReq_6" text="The requirement shall be feasible with the available resources and constraints, to ensure sustainable and effective development." base_Class="_-KwGoHQ7Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_ILcIcHQ8Ee6lW_W_zHdLeg" base_NamedElement="_ILaTQHQ8Ee6lW_W_zHdLeg" id="Req_QAReq_7" text="The requirement should be prioritized according to their importance, to support the expected order of releases and limit dependency risks." base_Class="_ILaTQHQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_NMj48HQ8Ee6lW_W_zHdLeg" base_NamedElement="_NMiDwHQ8Ee6lW_W_zHdLeg" id="Req_QAReq_8" text="The requirement shall be unambiguous, to prevent differing interpretations between stakeholder and development team." base_Class="_NMiDwHQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_Q-ItIHQ8Ee6lW_W_zHdLeg" base_NamedElement="_Q-G38HQ8Ee6lW_W_zHdLeg" id="Req_QAReq_9" text="The requirement should be comprehensible, to support a common understanding of the requirement between stakeholder and development team." base_Class="_Q-G38HQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_Vj-24HQ8Ee6lW_W_zHdLeg" base_NamedElement="_Vj9BsHQ8Ee6lW_W_zHdLeg" id="Req_QAReq_10" text="The requirement shall be testable, to support validation and verification activities." base_Class="_Vj9BsHQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_ZGvzAHQ8Ee6lW_W_zHdLeg" base_NamedElement="_ZGt90HQ8Ee6lW_W_zHdLeg" id="Req_QAReq_11" text="The requirement should be unique, to avoid duplicate planning of development efforts and inconsistencies between requirements." base_Class="_ZGt90HQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_czZq8HQ8Ee6lW_W_zHdLeg" base_NamedElement="_czX1wHQ8Ee6lW_W_zHdLeg" id="Req_QAReq_12" text="The requirement shall be identifiable, to support traceability and improve clear communication." base_Class="_czX1wHQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_0o_nEHQ8Ee6lW_W_zHdLeg" base_NamedElement="_0o9K0HQ8Ee6lW_W_zHdLeg" id="Req_QAReq_13" text="The requirement should be traceable to its origin, to support change management and stakeholder acceptance." base_Class="_0o9K0HQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_51TT4HQ8Ee6lW_W_zHdLeg" base_NamedElement="_51ResHQ8Ee6lW_W_zHdLeg" id="Req_QAReq_14" text="The requirement should be mapped to elements in the system, to support planning of development and effective use of resources." base_Class="_51ResHQ8Ee6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_c0avQHSXEe6lW_W_zHdLeg" base_NamedElement="_c0YTAHSXEe6lW_W_zHdLeg" id="Req_QAReq_15" text="The requirement may be conforming with defined formats to support understanding between stakeholder and development team." base_Class="_c0YTAHSXEe6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_dXyiEHSXEe6lW_W_zHdLeg" base_NamedElement="_dXws4HSXEe6lW_W_zHdLeg" id="Req_QAReq_16" text="The requirement shall be derived from stakeholder input, to avoid extraneous development efforts." base_Class="_dXws4HSXEe6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_d8H-MHSXEe6lW_W_zHdLeg" base_NamedElement="_d8Fh8HSXEe6lW_W_zHdLeg" id="Req_QAReq_17" text="The requirement should be analyzed by the development team, to understand its impact on the system and development." base_Class="_d8Fh8HSXEe6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_em3kUHSXEe6lW_W_zHdLeg" base_NamedElement="_em1vIHSXEe6lW_W_zHdLeg" id="Req_QAReq_18" text="The requirement should be reviewed by the stakeholder and the development team, to ensure informed agreement." base_Class="_em1vIHSXEe6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_3ZM1gHSXEe6lW_W_zHdLeg" base_NamedElement="_3ZLAUHSXEe6lW_W_zHdLeg" id="Req_QAReq_19" text="The requirement shall be agreed upon by the stakeholder and the development team, to ensure accountability and support the establishment of baselines." base_Class="_3ZLAUHSXEe6lW_W_zHdLeg"/>
  <Requirements:Requirement xmi:id="_5QJJAHSXEe6lW_W_zHdLeg" base_NamedElement="_5QHT0HSXEe6lW_W_zHdLeg" id="Req_QAReq_20" text="The requirement should be tangible" base_Class="_5QHT0HSXEe6lW_W_zHdLeg"/>
</xmi:XMI>
